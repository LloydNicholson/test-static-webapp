{
  "schema": "schema.graphql",
  "documents": "**/*.graphql",
  "extensions": {
    "strawberryShake": {
      "name": "DbClient",
      "url": "http://localhost:4280/data-api/graphql",
      "requestStrategy": "Default",
      "outputDirectoryName": null,
      "emitGeneratedCode": false,
      "razorComponents": true,
      "accessModifier": "public",
      "records": {
        "inputs": false,
        "entities": false
      },
      "transportProfiles": [
        {
          "default": "Http",
          "subscription": "WebSocket"
        }
      ]
    }
  }
}